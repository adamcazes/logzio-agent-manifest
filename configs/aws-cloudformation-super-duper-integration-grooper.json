{
   "name": "AWS",
   "logo": "https://dytvr9ot2sszz.cloudfront.net/logz-docs/shipper-logos/aws.svg",
   "description": "",
   "hint": "Select the environment where you want to deploy Logz.io Telemetry Collector.",
   "subtypes": [
     {
       "name": "aws-cloudformation-super-duper",
       "label": "AWS Cloudformation (SUPER)",
       "logo": "https://dytvr9ot2sszz.cloudfront.net/logz-docs/shipper-logos/aws_ec2_linux.png",
       "supportedOs": ["Linux"],
       "integrationTemplate": "mashu-mashu/yam-pam-po/maspik-nehmad/ten-li/latest&stackName=superduper-bertet&generalParam=<<LOGZIO_AGENT_PARAM_VALUE_generalParam>>",
       "datasources": [
         {
           "name": "System",
           "logo": "https://dytvr9ot2sszz.cloudfront.net/logz-docs/shipper-logos/local_host.png",
           "description": "Logz.io Uses OpenTelemetry to monitor your EC2.",
            "params": [
              {
                "type": "single-select",
                "name": "awsRegion",
                "label": "AWS Region",
                "description": "AWS Region",
                "required": true,
                "options": [
                  { "label": "us-east-1", "value": "us-east-1" },
                  { "label": "eu-central-1", "value": "eu-central-1" }
                ]
              },
              {
                "type": "text",
                "name": "envID",
                "label": "Environment Name",
                "description": "The name for your environment's identifier, to easily identify the telemetry data for each environment.",
                "hint": "By typing an environment name, the name will be added as a label to the telemetry data of your environment. Default value will be used if empty.",
                "value": ""
              },
              {
                "type": "boolean",
                "name": "isTaint",
                "label": "Taints and Tolerations",
                "description": "Taints allow a node to repel a series of pods. Tolerations are applied to pods and allow them to schedule onto nodes with matching taints. Enable to force deployment on all pods.",
                "hint": "By checking this option the Logz.io Telemetry Collector will be deployed on all nodes.",
                "value": true,
                "params": []
              },
              {
                "type": "text",
                "name": "generalParam",
                "label": "generalParam",
                "required": true
              }
            ],
           "telemetries": [
             {
               "type": "LOG_ANALYTICS",
               "hint": "Switch the toggle on if you want Logz.io Telemetry Collector to collect logs from your EC2.",
               "params":
               [
                 {
                   "type": "text",
                   "name": "param_bucketName",
                   "label": "Enter your bucket name",
                   "required": true,
                   "description": "The name of the bucket"
                 },
                 {
                   "type": "text",
                   "name": "param_logAnalytics1",
                   "label": "logAnalytics1 (text value)",
                   "required": true
                 }
               ]
             },
             {
               "type": "METRICS",
               "hint": "Switch the toggle on if you want Logz.io Telemetry Collector to collect metrics from your EC2.",
               "params": [
                 {
                   "type": "path-list",
                   "name": "param_pathList",
                   "label": "Path list (string[] value)",
                   "required": true
                 },
                 {
                   "type": "multiline-pattern",
                   "name": "param_multilinePatternn",
                   "label": "Multiline Pattern (object[] value)",
                   "required": true
                 },
                 {
                   "type": "key-value",
                   "name": "key-value-test1",
                   "required": true,
                   "label": "Key value param (object[] value)",
                   "description": "Key value param description",
                   "value": [
                     { "key": "first_key", "value":  "first_value"},
                     { "key": "second_key", "value":  "second_value"}
                   ]
                 }
               ],
               "dashboards": [
                 "2oLvCy5p914pM9m5pLoD6u"
               ]
             }
           ]
         }
       ]
     }
   ]
